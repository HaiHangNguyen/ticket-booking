<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Ticket_Backend::Ticket" title="Ticket" translate="title">
                    <resource id="Ticket_Directory::menu" title="Directory" translate="title" sortOrder="90">
                        <resource id="Ticket_Directory::city" title="City" translate="title" sortOrder="10"/>
                        <resource id="Ticket_Directory::district" title="District" translate="title" sortOrder="20"/>
                        <resource id="Ticket_Directory::ward" title="Ward" translate="title" sortOrder="30"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
